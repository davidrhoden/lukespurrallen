<h3>Watch me count to ten!</h3>
<ul>
  {% for num in range(1, 10) %}
    <li class="{{ cycle(['odd', 'even'], loop.index0) }}">
      {{ num }}
    </li>
  {% endfor %}
</ul>


{% for entry in craft.entries.section('shows') %}

{{ entry.date | date("F j, Y")}}
{ entry.venue.first() }}
{{ entry.lineUp }}
{{ entry.time | date("g:i a") }}
{% if entry.cover| length %}
{{ entry.cover }}
{% endif %}
{{ entry.body }}
{% set image = entry.showArt.first() %}
{% if image %}
<img class="albumart" src="{{ image.getUrl('sixHundredFortyPxWide') }}">
{% endif %}