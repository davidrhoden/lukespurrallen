{% extends "_layout" %}

{% block content %}
	<article>
		{% for asset in entry.poster %}
			<img src="{{ asset.getUrl() }}" />
		{% endfor %}
		<h1>{{ entry.title }}</h1>
		{{ entry.body }}
		<p>{{ entry.venue }}</p>
	</article>
{% endblock %}
