{% extends "_layout" %}
{% set title = "Luke Spurr Allen : {{ entry.title }}" %}
{% block content %}
	<div class="press-block">
	<article class="flex">
		<div class="press-text">
			{% for asset in entry.videos %}
				{{ entry.embedCode}}
			{% endfor %}<br>
			<h1>{{ entry.title }}</h1>
			<hr>
			{{ entry.body }}
		</div>
	</article>
	</div>
{% endblock %}
